{
    "name": "SFTP Example",
    "protocol": "sftp",
    "openSsh": true,
    "uploadOnSave": true,
    "useTempFile": false,
    "ignoreFile": ".gitignore",
    "ignore": [
        ".git",
        ".DS_Store"
    ],
    "sshConfigPath": "/path/to/ssh_config",
    "syncOption": {
        "delete": true,
        "update": true
    },
    "profiles": {
      "default": {
        "host": "HostName",
        "port": 22,
        "username": "UserName",
        "remotePath": "/remote/path"
      }
    }
}
